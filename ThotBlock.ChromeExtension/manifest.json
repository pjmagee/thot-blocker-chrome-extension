{
  "name": "ThotBlock",
  "description": "Removes streamers who walk the fine line of the twitch Terms of service when it comes to sexually suggestive content or nudity.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [ "activeTab", "scripting" ],
  "content_scripts": [
    {
      "matches": [ "http://*.twitch.tv/*" ],
      "js": [ "begone.js" ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}